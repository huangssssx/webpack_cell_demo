{
  "name": "edu-login",
  "version": "0.0.1",
  "description": "Make your `React` app work with `webpack` in `Internet Explorer 8.0`.",
  "repository": "git@github.com:liuhong1happy/react-webpack-ie8.git",
  "main": "js/app.js",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "es3ify-loader": "^0.2.0",
    "es3ify-webpack-plugin": "0.0.1",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "es5-shim": "^4.5.9",
    "jquery": "^1.12.4"
  },
  "scripts": {
    "start": "webpack-dev-server --devtool eval --progress --hot --host 0.0.0.0 --config webpack.dev.config.js",
    "build": "webpack --devtool cheap-module-source-map --progress --color --profile --config webpack.pro.config.js --watch"
  },
  "author": "Holly Liu"
}
